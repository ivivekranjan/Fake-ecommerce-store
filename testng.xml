<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="FakeStore API Suite" verbose="1" parallel="false">
  <listeners>
    <listener class-name="listeners.TestListener"/>
  </listeners>

  <test name="Smoke">
    <classes>
      <class name="tests.AuthTests"/>
      <class name="tests.ProductTests"/>
      <class name="tests.CartTests"/>
      <class name="tests.EndToEndFlowTest"/>
    </classes>
  </test>
</suite>
